PROGRAM TotalEnergy
    ! This program is intended to calculate the total
    ! energy of a particle, which equation is 
    ! E_total = E_potential + E_kinetic

    IMPLICIT NONE

    REAL :: E_total
    REAL :: E_potential
    REAL :: E_kinetic

    REAL :: mass
    REAL :: gravity
    REAL :: velocity
    REAL :: height

END PROGRAM TotalEnergy
